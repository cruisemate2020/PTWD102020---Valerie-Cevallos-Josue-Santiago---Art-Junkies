{{#if userInSession}}
    <h3>Welcome, {{userInSession.fname}} </h3>
    
    <form action="/profile" method="POST">
    <p>
      Login: {{userInSession.username}}
      <br>
      First Name: {{userInSession.fname}}
      <br>
      Last Name: {{userInSession.lname}}
      <br>
      Email: {{userInSession.email}}
      <br>
    
      </p>
      <br>
      <br>
    </form>
    

<form action="/logout" method="POST">
<button type="submit">Logout</button>
</form>
{{else}}
    <b>No user is logged in!<b>
{{/if}}

<form action="/profile-edit" method="GET">
<button type="submit">Edit Profile</button>
</form>

<form action="/upload" method="GET">
  <button type="submit">Start Uploading!</button>
</form>

<a href="/profile"> Refresh </a>